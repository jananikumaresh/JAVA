import java.util.*;

class Maps {

    public static void main(String args[]) {

        HashMap obj = new HashMap();

        obj.put("A", new Integer(1));

        obj.put("B", new Integer(2));

        obj.put("C", new Integer(3));

        System.out.println(obj);

    }

}



Question 30Answer

a.
{A-1, B-1, C-1}


b.
{A=1, B=2, C=3}


c.
{A 1, B 1, C 1}


d.
{A, B, C}

